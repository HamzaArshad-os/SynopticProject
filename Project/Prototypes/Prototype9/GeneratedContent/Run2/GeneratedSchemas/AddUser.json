{
  "required": [
    "email",
    "first_name",
    "last_name",
    "password"
  ],
  "type": "object",
  "properties": {
    "first_name": {
      "type": "string",
      "example": "Ashley"
    },
    "last_name": {
      "type": "string",
      "example": "Williams"
    },
    "email": {
      "type": "string",
      "format": "email",
      "example": "ashley.williams@mmu.ac.uk"
    },
    "password": {
      "type": "string",
      "example": "hello123"
    }
  }
}